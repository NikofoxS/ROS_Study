<launch>

    <rosparam file="$(find gp7_moveit_config)/config/sim_gazebo_conf/gp7_trajectory_control.yaml" command="load"/>

    <node name="gp7_yaskawa_controller_spawner" pkg="controller_manager" type="controller_manager" respawn="false"
          output="screen" ns="/gp7_yaskawa" args="load gp7_yaskawa_controller"/>

    <node name="gp7_joint_group_position_controller_loader" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/gp7_yaskawa" args="gp7_joint_group_position_controller"/>spawner
</launch>
